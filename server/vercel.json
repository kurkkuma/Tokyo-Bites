{
  "builds": [
    {
      "src": "*.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "/"
    },
    {
      "src": "/menu",
      "dest": "/menu.html"
    },
    {
      "src": "/about",
      "dest": "/about.html"
    },
    {
      "src": "/favorite",
      "dest": "/favorite.html"
    },

    {
      "src": "/basket",
      "dest": "/basket.html"
    },
    {
      "src": "/products",
      "dest": "/products"
    },
    {
      "src": "/reviews",
      "dest": "/reviews"
    },
    {
      "src": "/add-user",
      "dest": "/add-user"
    },
    {
      "src": "/update-favorite",
      "dest": "/update-favorite"
    },
    {
      "src": "/add-review",
      "dest": "/add-review"
    }
  ]
}
